    },
    "autoload": {
        "psr-4": {
            "Waad\\Observer\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Waad Mawlood",
            "email": "waad_mawlood@outlook.com",
            "homepage": "https://waad.netlify.app",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "extra": {
            "sort-packages": true
        }
    }
