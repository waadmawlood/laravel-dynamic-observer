{
    "name": "waad/laravel-dynamic-observer",
    "description": "Call observer of the model without requiring in any provider, support multi observers",
    "type": "library",
    "keywords": [
        "laravel",
        "laravel-observer",
        "model",
        "model-observer",
        "observer"
    ],
    "require": {
        "php": ">=7.3"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0",
        "pestphp/pest-plugin-laravel": "^1.0|^2.0"
    },
    "autoload": {
        "psr-4": {
            "Waad\\Observer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Waad\\Observer\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Observers\\": "workbench/app/Observers/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Waad Mawlood",
            "email": "waad_mawlood@outlook.com",
            "homepage": "https://waad.netlify.app",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "test": "vendor/bin/pest"
    }
}
